{
    "name": "heintzel/code-breaker",
    "type": "project",
    "license": "MIT",
    "version": "0.3.0",
    "autoload": {
        "psr-4": {
            "CodeBreaker\\": "app/"
        }
    },
    "scripts": {
        "phpmd" : "vendor/bin/phpmd app text phpmd.xml",
        "phpcs" : "vendor/bin/phpcs --extensions=php --standard=PSR12 app",
        "phpcs fix" : "vendor/bin/phpcbf --extensions=php --standard=PSR12 app"
    },
    "require": {},
    "require-dev": {
        "squizlabs/php_codesniffer": ">=3.7.1",
        "phpmd/phpmd": ">=2.13.0",
        "phpunit/phpunit": ">=9.5.28"
    }
}
